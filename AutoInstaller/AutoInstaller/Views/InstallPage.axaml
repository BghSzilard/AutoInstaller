<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:AutoInstaller.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AutoInstaller.Views.InstallPage"
			 x:DataType="vm:InstallViewModel"
			 x:CompileBindings="True">
	<UserControl.DataContext>
		<vm:InstallViewModel></vm:InstallViewModel>
	</UserControl.DataContext>
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"></RowDefinition>
			<RowDefinition Height="Auto"></RowDefinition>
		</Grid.RowDefinitions>
		<ListBox Grid.Row="0" ItemsSource="{Binding Programs}"></ListBox>
		<Button Grid.Row="1" Content="Install" Command="{Binding InstallProgramCommand}"></Button>
	</Grid>
</UserControl>
